{
  "name": "react-flux-graphql-demo",
  "version": "1.0.0",
  "description": "React and Flux based data-driven application built with GraphQL",
  "main": "lib/data/server.js",
  "scripts": {
    "dev": "pm2 flush && pm2 start ecosystem.config.js --env development",
    "webpack": "webpack -wd",
    "test": "NODE_PATH=./lib jest --watch",
    "verify-tests": "NODE_PATH=./lib jest --coverage",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files",
    "start-prod": "NODE_ENV=production NODE_PATH=./build pm2 start ecosystem.config.js --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aviralmishra/react-flux-graphql-demo.git"
  },
  "keywords": [
    "React",
    "ES6",
    "Jest",
    "Enzyme",
    "Webpack",
    "Flux",
    "React Router"
  ],
  "author": "Aviral Mishra (aviral.mishra@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aviralmishra/react-flux-graphql-demo/issues"
  },
  "homepage": "https://github.com/aviralmishra/react-flux-graphql-demo#readme",
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "open-browser-webpack-plugin": "^0.0.5",
    "pm2": "^2.9.1",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^3.3.7",
    "ejs": "^2.5.7",
    "events": "^1.1.1",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "flux": "^3.1.3",
    "graphql": "^0.12.3",
    "graphql-relay": "^0.5.4",
    "humps": "^2.0.1",
    "jquery": "^3.2.1",
    "kerberos": "0.0.23",
    "lodash.groupby": "^4.6.0",
    "lodash.map": "^4.6.0",
    "lodash.pickby": "^4.6.0",
    "moment": "^2.20.1",
    "mongodb": "^3.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  }
}
